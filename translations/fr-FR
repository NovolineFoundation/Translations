command.gen_messages.usage_1=.%s list - liste les messages
command.gen_messages.usage_2=.%s clear - retire les messages
command.gen_messages.usage_3=.%s add <message> - ajoute une message
command.gen_messages.usage_4=.%s remove <index> - retire un message
command.gen_messages.usage_5=.%s browse - ouvre l'endroit ou est stoqué le message

command.gen_messages.usage_ph=Variables utilisables dans les messages:
command.gen_messages.usage_ph_1= {c_version} - version du client
command.gen_messages.usage_ph_2= {c_build} - date de sortie du client
command.gen_messages.usage_ph_3= {u_username} - votre nom d'utilisateur
command.gen_messages.usage_ph_4= {u_uid} - vutre uid

command.killsults.ph_1= {player} - le nom du joueur

