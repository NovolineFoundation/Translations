command.gen_messages.usage_2=.%s clear - clear messages
command.gen_messages.usage_3=.%s add <message> - add a message
command.gen_messages.usage_4=.%s remove <index> - remove a message
command.gen_messages.usage_5=.%s browse - open message file location

command.gen_messages.usage_ph=Placeholders in messages:
command.gen_messages.usage_ph_1= {c_version} - client version
command.gen_messages.usage_ph_2= {c_build} - client build date
command.gen_messages.usage_ph_3= {u_username} - your username
command.gen_messages.usage_ph_4= {u_uid} - your uid

